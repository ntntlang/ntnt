import { keys, values, entries, has_key } from "std/collections"

let sites = map {
    "bear_lake": map { "name": "Bear Lake", "url": "https://example.com/bear" },
    "snowbird": map { "name": "Snowbird", "url": "https://example.com/snow" },
    "milld": map { "name": "MILL-D", "url": "https://example.com/mill" }
}

print("Keys:")
print(keys(sites))

print("\nValues:")
print(values(sites))

print("\nEntries:")
print(entries(sites))

print("\nHas key 'bear_lake'?")
print(has_key(sites, "bear_lake"))

print("\nHas key 'invalid'?")
print(has_key(sites, "invalid"))

print("\nIterate over keys:")
for key in keys(sites) {
    let site = sites[key]
    print("Site " + key + " = " + str(site["name"]))
}
