{
  "name": "ntnt-lang",
  "displayName": "NTNT Language (Intent)",
  "description": "Syntax highlighting and language support for the NTNT (Intent) programming language",
  "version": "0.1.0",
  "publisher": "ntnt-lang",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshcramer/ntnt"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "ntnt",
    "intent",
    "programming language",
    "contracts",
    "design by contract",
    "AI"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ntnt",
        "aliases": [
          "NTNT",
          "Intent",
          "ntnt"
        ],
        "extensions": [
          ".tnt"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/intent-light.png",
          "dark": "./icons/intent-dark.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "ntnt",
        "scopeName": "source.ntnt",
        "path": "./syntaxes/ntnt.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "ntnt",
        "path": "./snippets/ntnt.json"
      }
    ]
  },
  "icon": "icons/intent-logo.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  }
}
